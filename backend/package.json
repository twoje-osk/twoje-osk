{
  "name": "@osk/backend",
  "private": true,
  "version": "0.0.1",
  "scripts": {
    "dev": "nest start --watch",
    "dev:debug": "nest start --debug --watch",
    "start": "nest start",
    "build": "nest build",
    "start:prod": "node dist/src/main",
    "prebuild": "rimraf dist",
    "migrate": "typeorm-ts-node-esm migration:run -d src/data-source.ts",
    "copy-client": "cp -R ../frontend/dist ./client",
    "admin:dev": "nest start --config nest-cli.admin.json --watch",
    "admin:start": "nest start --config nest-cli.admin.json",
    "admin:build": "nest build --config nest-cli.admin.json",
    "admin:start:prod": "node dist/src/admin",
    "admin:build-entities": "ts-node scripts/createAdminEntities.ts",
    "type-check": "tsc --noEmit",
    "lint": "eslint src --ext ts",
    "typeorm": "typeorm-ts-node-esm -d src/data-source.ts",
    "create-migration": "typeorm-ts-node-esm migration:generate -d src/data-source.ts",
    "create-empty-migration": "typeorm-ts-node-esm migration:create",
    "seed": "ts-node ./src/seeds/seed.ts"
  },
  "dependencies": {
    "@adminjs/express": "^5.0.0",
    "@adminjs/nestjs": "^5.0.0",
    "@adminjs/passwords": "^3.0.0",
    "@adminjs/typeorm": "^4.0.0",
    "@faker-js/faker": "^6.3.1",
    "@nestjs/common": "^8.4.4",
    "@nestjs/config": "^2.0.0",
    "@nestjs/core": "^8.0.0",
    "@nestjs/jwt": "^8.0.0",
    "@nestjs/mapped-types": "^1.0.1",
    "@nestjs/passport": "^8.2.1",
    "@nestjs/platform-express": "^8.4.7",
    "@nestjs/serve-static": "^2.2.2",
    "@nestjs/swagger": "^5.2.1",
    "@nestjs/typeorm": "^8.0.3",
    "@osk/eslint-plugin": "1.0.0",
    "@osk/shared": "1.0.0",
    "@types/bcrypt": "^5.0.0",
    "@types/react-big-calendar": "^0.38.1",
    "adminjs": "^6.0.0",
    "bcrypt": "^5.0.1",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.13.2",
    "date-fns": "^2.28.0",
    "dotenv": "^16.0.0",
    "esbuild": "^0.14.36",
    "express": "^4.18.1",
    "express-formidable": "^1.2.0",
    "express-session": "^1.17.3",
    "glob": "^8.0.3",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-local": "^1.0.0",
    "pg": "^8.7.3",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.2.0",
    "swagger-ui-express": "^4.4.0",
    "typeorm": "^0.3.6",
    "typeorm-transactional-cls-hooked": "^0.1.21"
  },
  "devDependencies": {
    "@nestjs/cli": "^8.0.0",
    "@nestjs/schematics": "^8.0.0",
    "@nestjs/testing": "^8.0.0",
    "@types/express": "^4.17.13",
    "@types/glob": "^7.2.0",
    "@types/node": "^16.0.0",
    "@types/passport-jwt": "^3.0.6",
    "@types/passport-local": "^1.0.34",
    "@types/supertest": "^2.0.11",
    "@typescript-eslint/eslint-plugin": "^5.13.0",
    "@typescript-eslint/parser": "^5.0.0",
    "eslint": "^8.2.0",
    "eslint-config-airbnb-base": "15.0.0",
    "eslint-config-airbnb-typescript": "^17.0.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-plugin-import": "^2.25.2",
    "eslint-plugin-prettier": "^4.0.0",
    "prettier": "2.6.2",
    "source-map-support": "^0.5.20",
    "ts-loader": "^9.2.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^3.10.1",
    "typescript": "^4.7.4",
    "webpack": "^5.72.0"
  }
